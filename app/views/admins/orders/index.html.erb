<div class="contianer">
    <div class="row">
    　　<div class="col-md-7 col-md-offset-1">
       　　 <h2>注文一覧画面</h2>
        　　<table class="table table-hover table-inverse">
        　<thead>
          <tr>
            <th>購入日時</th>
            <th>購入者</th>
            <th>注文個数</th>
            <th>注文ステータス</th>
          </tr>
        </thead>
        <% @orders.each do |order| %>
        <tbody>
          <tr>
          
          <th><%= link_to order.created_at.to_s(:datetime_jp), admins_order_path(order.id) %></th>
          <th><%= order.customer.last_name %> <%= order.customer.first_name %></th>
          
            <th>
              <%= order.order_details.sum(:quantity) %>
              </th>
          <th><%= order.order_status %></th>
          </tr>
        </tbody>
      <% end %>
     </table>
     </div>
  </div>
</div>