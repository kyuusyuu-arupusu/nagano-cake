<div class="container">
<div class="row">
  <div class="col-xs-2"></div>
  <div class="col-xs-8">

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field">
    <%= f.label :名前（姓・名） %>
    <%= f.text_field :last_name, autocomplete: "last_name" %>
    <%= f.text_field :first_name, autocomplete: "first_name" %>
  </div><br>

  <div class="field">
    <%= f.label :名前（カナ姓・カナ名） %>
    <%= f.text_field :last_name_kana, autocomplete: "last_name_kana" %>
    <%= f.text_field :first_name_kana, autocomplete: "first_name_kana" %>
  </div><br>

  <div class="field">
    <%= f.label :郵便番号 %>
    <%= f.text_field :post_code, autocomplete: "post_code" %>
  </div><br>

  <div class="field">
    <%= f.label :住所 %>
    <%= f.text_field :address, autocomplete: "address" %>
  </div><br>

   <div class="field">
    <%= f.label :電話番号 %>
    <%= f.telephone_field :phone_number, autofocus: true, autocomplete: "phone_number" %>
  </div><br>

  <div class="field">
    <%= f.label :メールアドレス %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div><br>

  <div class="field">
    <%= f.label :パスワード %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %>
    <%= f.password_field :password, autocomplete: "new-password" %>
  </div><br>

  <div class="field">
    <%= f.label :パスワード確認 %>
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %>
  </div><br>

  <div class="actions">
    <%= f.submit "登録", class:"btn btn-info" %>
  </div>
<% end %>

<%= render "customers/shared/links" %>
</div>
<div class="col-xs-2"></div>

