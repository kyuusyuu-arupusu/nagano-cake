
<div class="container">
  <div class="row">
     <div class="col-md-4 col-md-offset-4">


        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= render "devise/shared/error_messages", resource: resource %>
  <tbody>
  <tr>
  <div class="form-group row">
    <div class="field">
      <td><%= f.label :名前（姓・名） %></td></br>
      <td><div class="col-md-6">
          <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name", class: "form-control" %>
          </div>
          <div class="col-md-6">
          <%= f.text_field :first_name, autocomplete: "first_name", class: "form-control form-inline" %>
          </div>
      </td>
    </div>
 </div></tr>
  
 <div class="form-group row">
  <tr><div class="field">
  <td>
     <%= f.label :名前（カナ姓・カナ名） %></td></br>
  <td><div class="col-md-6">
      <%= f.text_field :last_name_kana, autocomplete: "last_name_kana", class: "form-control form-inline" %>
      </div>
      <div class="col-md-6">
      <%= f.text_field :first_name_kana, autocomplete: "first_name_kana", class: "form-control form-inline" %></td>
      </div> 
      </div><tr>
  </div>

  <tr>
   <div class="field">
      <td><%= f.label :郵便番号 %></td></br>
      <td><%= f.text_field :post_code, autocomplete: "post_code", class: "form-control" %></td>
   </div>
  <tr>

  <tr>
    <div class="field">
      <td><%= f.label :住所 %></td></br>
      <td><%= f.text_field :address, autocomplete: "address", class: "form-control" %></td>
    </div>
  </tr>

  <tr>
    <div class="field">
      <td><%= f.label :電話番号 %></td></br>
      <td><%= f.telephone_field :phone_number, autocomplete: "phone_number", class: "form-control" %></td>
    </div>
  </tr>

  <tr>
    <div class="field">
      <td><%= f.label :メールアドレス %></td></br>
      <td><%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control" %></td>
    </div>
  </tr>
  
  <tr>
    <div class="field">
      <td><%= f.label :パスワード %>
      <% if @minimum_password_length %>
      <em>(<%= @minimum_password_length %> characters minimum)</em></td></br>
      <% end %>
      <td><%= f.password_field :password, autocomplete: "new-password", class: "form-control" %></td>
    </div>
  </tr>

  <tr>
    <div class="field">
      <td><%= f.label :パスワード確認 %></td></br>
      <td><%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-control" %>
      </td>
    </div>
  </tr>
  <tr>
  <td><div class="actions">
    <%= f.submit "登録", class:"btn btn-info form-control" %>
  </td>
  </div>
  </tr>
  </tbody>
  </table>
<% end %>
<%= render "customers/shared/links" %>
</div>
</div>
<div class="col-xs-2"></div>

 <li class="">
              